{
    "name": "ballstatecber/datacenter-plugin-cakephp4",
    "description": "Ball State CBER DataCenter plugin for CakePHP 4",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "php": ">=7.4",
        "cakephp/cakephp": "~4.1.0",
        "ext-json": "*",
        "twbs/bootstrap": "^4.0"
    },
    "require-dev": {
        "cakephp/bake": "^2.0.3",
        "cakephp/cakephp-codesniffer": "~4.2.0",
        "phpunit/phpunit": "~8.5.0"
    },
    "autoload": {
        "psr-4": {
            "DataCenter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DataCenter\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
